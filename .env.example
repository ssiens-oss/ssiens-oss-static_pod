# ========================================
# StaticWaves POD - Environment Configuration
# ========================================

# ----------------
# TikTok OAuth
# ----------------
TIKTOK_CLIENT_ID=your_client_id_here
TIKTOK_CLIENT_SECRET=your_client_secret_here

# ----------------
# SaaS Licensing
# ----------------
LICENSE_KEY=SWPOD-XXXXXXXXXXXX
LICENSE_SALT=your_random_salt_here

# ----------------
# White-Label Branding
# ----------------
SAAS_BRAND=StaticWaves
SAAS_COLOR=#7F5BFF
SAAS_LOGO=/logo.png
SAAS_TAGLINE=TikTok POD Engine
SAAS_SUPPORT_EMAIL=support@staticwaves.io
SAAS_DOCS_URL=https://docs.staticwaves.io

# ----------------
# Optional: Theme Colors
# ----------------
SAAS_SECONDARY_COLOR=#F97316
SAAS_ACCENT_COLOR=#10B981
SAAS_BG_COLOR=#FFFFFF
SAAS_TEXT_COLOR=#1F2937

# ----------------
# Printify Integration
# ----------------
PRINTIFY_API_KEY=pk_live_your_api_key_here
PRINTIFY_SHOP_ID=your_shop_id_here
PRINTIFY_QUEUE_DIR=/opt/staticwaves_pod/queue

# ----------------
# Notifications
# ----------------
# Telegram (optional)
TELEGRAM_BOT_TOKEN=your_bot_token_here
TELEGRAM_CHAT_ID=your_chat_id_here

# Discord (optional)
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/your_webhook_url

# Slack (optional)
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/your_webhook_url

# ----------------
# Optional: Frontend (from original project)
# ----------------
GEMINI_API_KEY=your_gemini_key_here
