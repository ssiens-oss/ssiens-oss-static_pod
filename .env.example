# StaticWaves POD Studio - Environment Configuration

# Application
NODE_ENV=production
PORT=80

# Beta Testing
BETA_MODE=true
BETA_USERS_ALLOWED=true
FEATURE_FLAG_BATCH_MODE=true
FEATURE_FLAG_EDITOR=true

# API Configuration (if backend is added later)
# API_URL=http://localhost:3001
# GEMINI_API_KEY=your_gemini_api_key_here

# RunPod Configuration
RUNPOD_POD_ID=
RUNPOD_GPU_ID=

# Logging
LOG_LEVEL=info
ENABLE_ANALYTICS=false

# Performance
MAX_CONCURRENT_PROCESSES=10
CACHE_ENABLED=true

# Security
CORS_ORIGIN=*
RATE_LIMIT_ENABLED=false
