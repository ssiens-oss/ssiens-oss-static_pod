{
  "name": "staticwaves-pod-studio",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:music": "vite --config vite.music.config.ts",
    "dev:server": "tsx watch server.ts",
    "build": "vite build",
    "build:music": "vite build --config vite.music.config.ts",
    "build:production": "vite build --mode production",
    "preview": "vite preview",
    "start": "node dist-server/server.js",
    "start:production": "npm run build:production && tsx server.ts",
    "pipeline:single": "node scripts/run-pipeline.js",
    "pipeline:batch": "node scripts/run-batch.js",
    "setup:platforms": "node scripts/setup-platforms.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.22.1",
    "lucide-react": "^0.556.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/node": "^22.14.0",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react": "^5.0.0",
    "tsx": "^4.21.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
